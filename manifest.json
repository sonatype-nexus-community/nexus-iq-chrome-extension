{
    "name": "Sonatype Nexus IQ Evaluation",
    "description": "Sonatype Nexus IQ Evaluation-community developed",
    "author": "Cameron Townshend - ctownshend@sonatype.com",
    "version": "1.7.14",
    "manifest_version": 2,
    "permissions": [
      "storage",
      "activeTab",
      "declarativeContent",
      "webRequest",
      "webRequestBlocking", 
      "cookies", 
      "background",
      "tabs", 
      "https://search.maven.org/",
      "https://mvnrepository.com/",
      "https://www.npmjs.com/",
      "https://www.nuget.org/",
      "https://rubygems.org/",
      "https://pypi.org/",
      "https://packagist.org/",
      "https://cran.r-project.org/",
      "https://crates.io/",      
      "https://search.gocenter.io/",
      "https://ossindex.sonatype.org/",
      "https://github.com/",
      "*://*/*"

      ],
    "background": {
      "scripts": ["Scripts/jquery-3.3.1.min.js", "Scripts/utils.js", "Scripts/background.js"],
      "persistent": true
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
         "16": "images/SON_logo_favicon.png",
         "32": "images/SON_logo_favicon.png",
         "48": "images/SON_logo_favicon.png",
         "128": "images/SON_logo_favicon.png"
       }
     }, 
    "icons": {
      "16": "images/SON_logo_favicon.png",
      "32": "images/SON_logo_favicon.png",
      "48": "images/SON_logo_favicon.png",
      "128": "images/SON_logo_favicon.png"
    },
    


    
  "options_page": "options.html"    
  }
  