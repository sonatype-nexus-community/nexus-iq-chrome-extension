        "@sonatype/nexus-iq-api-client": "^0.156.3",
        "@sonatype/ossindex-api-client": "^2023.5.2",
        "@sonatype/react-shared-components": "12.16.2",
        "@types/chrome": "^0.0.218",
        "@types/firefox-webext-browser": "^109.0.0",
        "cash-dom": "^8.1.0",
        "classnames": "^2.3.2",
        "localforage": "^1.10.0",
        "node-window-polyfill": "^1.0.2",
        "packageurl-js": "^1.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "ts-deepmerge": "^6.1.0",
        "web-vitals": "^1.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.21.0",
        "@types/node": "^20.2.5",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
