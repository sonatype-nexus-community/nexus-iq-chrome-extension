{
  "name": "chrome-extension-nexus-iq",
  "version": "0.0.1-semantic-release",
  "description": "Chrome Extension for IQ Validation",
  "main": "background.js",
  "scripts": {
    "test": "jest --runInBand",
    "semantic-release": "semantic-release",
    "doc": "jsdoc -c jsdoc.json",
    "scan-ossi": "auditjs ossi",
    "scan-iq": "auditjs iq -s develop -a chrome-extension-nexus-iq -u admin -p admin123 -h http://iq-server:8070"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "setupFiles": [
      "jest-webextension-mock"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sonatype-nexus-community/nexus-iq-chrome-extension.git"
  },
  "keywords": [
    "IQ",
    "Validation",
    "Nexus",
    "Sonatype",
    "Lifecycle"
  ],
  "author": "Cameron Townshend",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ctownshend/chrome-extension-nexus-iq/issues"
  },
  "homepage": "https://github.com/ctownshend/chrome-extension-nexus-iq#readme",
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@semantic-release/github": "^5.5.5",
    "auditjs": "^4.0.10",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "jest-webextension-mock": "^3.5.0",
    "jsdoc": "^3.6.3",
    "puppeteer": "^2.0.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "semantic-release": "^15.13.31",
    "semantic-release-chrome": "^1.1.3"
  },
  "dependencies": {}
}
